<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.eightbitcloud.internode" android:versionName="2.00"
	android:versionCode="4">
	<application android:icon="@drawable/nodedroid"
		android:label="@string/app_name" android:theme="@android:style/Theme.Light">
		<activity android:name=".NodeUsage" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name=".AccountListActivity" android:label="@string/accountlist">
			<intent-filter>
				<category android:name="android.intent.category.PREFERENCE" />
				<action android:name="android.intent.action.INSERT" />
				<action android:name="android.intent.action.VIEW" />
			</intent-filter>
		</activity>


		<meta-data android:value="a14c806b710009e" android:name="ADMOB_PUBLISHER_ID" />
		<service android:name="DataFetchService" android:enabled="true"
			android:exported="false"></service>
	</application>



	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-sdk android:minSdkVersion="4" android:targetSdkVersion="4"></uses-sdk>
</manifest> 